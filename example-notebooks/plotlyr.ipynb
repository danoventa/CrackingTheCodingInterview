    "library(IRdisplay)\n",
    "\n",
    "data <- list(list(x=c(1999, 2000, 2001, 2002), y=c(10, 15, 13, 17), type='scatter'))\n",
    "figure <- list(data=data)\n",
    "\n",
    "mimebundle <- list('application/vnd.plotly.v1+json'=figure)\n",
    "#mimebundle[[]] <- figure\n",
    "IRdisplay::publish_mimebundle(mimebundle)"
